{
  "name": "suspense",
  "version": "0.0.10",
  "description": "Utilities for working with React suspense",
  "author": "Brian Vaughn <brian.david.vaughn@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bvaughn/suspense.git"
  },
  "source": "src/index.ts",
  "main": "dist/suspense.js",
  "module": "dist/suspense.module.js",
  "types": "dist/suspense.d.ts",
  "scripts": {
    "build": "parcel build",
    "test": "pnpm run test:browser & pnpm run test:node",
    "test:browser": "jest",
    "test:browser:watch": "jest --watch",
    "test:node": "node --expose-gc node_modules/jest/bin/jest.js --config=jest.config.node.js",
    "test:node:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --watch",
    "test:node:watch": "node --expose-gc node_modules/jest/bin/jest.js --config=jest.config.node.js --watch",
    "test:watch": "jest --watch",
    "watch": "parcel watch"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "extra-bigint": "^1.1.10",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "react": "latest",
    "react-dom": "latest",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "interval-utilities": "workspace:^",
    "point-utilities": "workspace:^"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "browserslist": [
    "Chrome 79"
  ]
}
